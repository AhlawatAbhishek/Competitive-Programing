"": {
  "prefix": "binpow",
  "body": [
    "long long binpow(long long a, long long b, ll mod) {",
    "    ll res = 1;",
    "    while(b){",
    "        if((b & 1) != 0){",
    "            res = ((res % mod) * (a % mod))%mod;",
    "        }",
    "        a = (a % mod * a % mod)%mod;",
    "        b >>=1;",
    "    }",
    "    return res;",
    "}"
  ],
  "description": ""
}
